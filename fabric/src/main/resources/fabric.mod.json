{
  "schemaVersion": 1,
  "id": "autoelytra",
  "version": "${version}",
  "name": "Auto Elytra",
  "description": "A client side mod that automatically switches your elytra and chestplate when you go to fly.",
  "authors": [
    "NimajnebEC"
  ],
  "contact": {
    "sources": "https://github.com/NimajnebEC/auto-elytra",
    "issues": "https://github.com/NimajnebEC/auto-elytra/issues"
  },
  "license": "MIT",
  "icon": "assets/autoelytra/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": ["ec.brooke.autoelytra.fabric.AutoElytraFabric"]
  },
  "mixins": [
    "autoelytra.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "architectury": ">=11.1.17",
    "minecraft": "1.20.5"
  }
}
